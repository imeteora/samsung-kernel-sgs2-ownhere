#!/ownhere/bin/busybox sh

if [ $# -lt 1 ];
then
  /system/bin/getprop
  for i in $(/ownhere/bin/busybox ls -1 /customkernel/property/*); do
    j=$(echo "$i" | cut -d "/" -f 4)
    k=$(cat $i);
    echo [$j]: [$k];
  done;
else
  if [ -f /customkernel/property/$1 ];
  then
    cat /customkernel/property/$1
  else
    /system/bin/getprop "$@"
  fi;
fi;
